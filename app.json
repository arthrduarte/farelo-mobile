{
  "expo": {
    "name": "Farelo",
    "slug": "farelo",
    "version": "1.5.2",
    "orientation": "portrait",
    "owner": "arthrduarte",
    "icon": "./assets/images/icon.png",
    "scheme": "farelo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.arthrduarte.farelo",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "associatedDomains": ["applinks:usefarelo.com"]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon_android.png",
        "backgroundColor": "#EDE4D2"
      },
      "minSdkVersion": 26,
      "package": "com.arthrduarte.farelo",
      "softwareKeyboardLayoutMode": "pan",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "usefarelo.com",
              "pathPrefix": "/"
            }
          ],
          "categories": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "androidNavigationBar": {
      "backgroundColor": "#EDE4D2",
      "barStyle": "dark-content"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#EDE4D2"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26
          }
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.791951088468-jql23hqupnu5eudogm3rrc8h634f6c8h"
        }
      ],
      ["expo-apple-authentication"]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "18f4d462-086e-4cef-916b-0594a32aca88"
      }
    }
  }
}
